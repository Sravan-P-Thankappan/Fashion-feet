    <div class="login-register-area pt-115 pb-120">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 col-md-12 ms-auto me-auto">
                    <div class="login-register-wrapper">
                        <div class="login-register-tab-list nav">
                            <a class="active" data-bs-toggle="tab" href="#lg1">
                                <h4> login </h4>
                            </a>
                            <a data-bs-toggle="tab" href="#lg2">
                                <h4> register </h4>
                            </a>
                        </div>
                        <div class="tab-content">
                            <div id="lg1" class="tab-pane active">
                                <div class="login-form-container">
                                    <div class="login-register-form">

                                        {{!-- login --}}
                                        
                                        {{#if block}}
                                        <label class="text-danger" for=""> This user is blocked</label>
                                        {{/if}}

                                        {{#if loginEr}}

                                        <p class="text-danger">Invalid Username or Password</p>

                                        {{/if}}
                                        <form action="/login" method="post">

                                            <input type="email" id="email" name="Email" placeholder="Email">

                                            <input type="password" id="lpassword" name="Password" placeholder="Password">

                                            <div class="button-box">
                                                <div class="login-toggle-btn">
                                                    {{!-- <input type="checkbox">
                                                    <label>Remember me</label> --}}
                                                    <a href="/otp">Forgot Password? Login With OTP</a>
                                                </div>
                                                <button type="submit">Login</button>
                                            </div>
                                        </form>



                                    </div>
                                </div>
                            </div>

                            <div id="lg2" class="tab-pane">
                                <div class="login-form-container">
                                    <div class="login-register-form">

                                        {{!-- register --}}
                                        {{#if ex}}
                                        <label for="" class="text-danger">Email Id is Already Existing</label>
                                        {{/if}}
                                        <form action="/register" method="post">
                                            {{!-- <input type="text" name="user-name" placeholder="Username"> --}}
                                            
                                             <label class="text-danger" for="" id="nameEr"></label>
                                            <input type="text" id="name" name="Name" onkeyup="validateName()"
                                                placeholder="Name">
                                                
                                            
                                               <label class="text-danger" for="" id="emailEr"></label>
                                               <input type="email" id="mail" name="Email" onkeyup="validateEmail()"
                                                placeholder="Email">
                                                
                                       
                                               <label class="text-danger" for="" id="phoneEr"></label>
                                                <input type="number" id="phone" name="Phone" onkeyup="validatePhone()"
                                                placeholder="Phone Number">
                                                
                                               
                                               <label class="text-danger"  for="" id="passwordEr"></label>                                               
                                              
                                                   <input type="password" id="password" name="Password"
                                                  onkeyup="validatePassword()" placeholder="Password">

                                                  <label class="text-success" for="" id="">Optional</label>
                                            <input type="text" id="rcode" name="Rcode"
                                                 placeholder="Referal Code   ">
                                           

                                                 <div class="button-box">
                                                <button type="submit" onclick=" return validateForm()">Register</button>
                                                <label class="text-danger" for="" id="submitEr"></label>
                                              
                                            </div>
                                        </form>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
